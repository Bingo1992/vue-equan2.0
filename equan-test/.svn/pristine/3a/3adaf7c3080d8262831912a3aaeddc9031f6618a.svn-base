e":"static/js/10.92ea9acf9c08c0d48443.js","sourcesContent":["<template>\r\n  <div>\r\n    <div class=\"redpacket-top\">\r\n        <img src=\"../../assets/images/coin.png\" alt=\"\">\r\n       <p>当前e币总额</p>\r\n       <h1>{{ebiTotal}}</h1>\r\n    </div>\r\n\r\n    <ul class=\"flex-layout redPacket-box\">\r\n        <li>\r\n          <a>\r\n            <p class=\"font-gray\">即将到期的e币</p>\r\n            <h3>{{currentEbi}}</h3>\r\n          </a>\r\n        </li>\r\n        <li>\r\n          <a>\r\n            <p class=\"font-gray\">当前e购总额</p>\r\n            <h3>{{currentEbuy}}</h3>\r\n          </a>\r\n        </li>\r\n    </ul>\r\n\r\n    <div style=\"margin-top: -40px\">\r\n        <h5 class=\"center-text pd border-bottom\">红包兑换</h5>\r\n\r\n        <div class=\"list-box redPacket-exchange\">\r\n          <div class=\"list-info-v\">\r\n              <p class=\"list-info-h\">\r\n                e币 <input class=\"inputText\" v-model=\"ebiInput\" type=\"number\" placeholder=\"请输入e币数量\">\r\n              </p>\r\n              <p class=\"list-info-h\">\r\n                e购 <input class=\"inputText\" v-model=\"ebuyInput\" type=\"number\" placeholder=\"请输入e购数量\" \r\n               >\r\n              </p>\r\n          </div>\r\n          <i class=\"icon-18\"></i>\r\n          <div class=\"list-info-h\">\r\n              <p>红包 <span class=\"font-orange redpacket\">{{getRedPacket}}</span>元</p>\r\n          </div>\r\n        </div>\r\n        <div class=\"btn\">\r\n          <p :class=\"['btn-pure-theme', getRedPacket <= 0?'disbtn':'']\" @click=\"gotoExchange()\">确定支付</p>\r\n        </div>\r\n   \r\n    </div>\r\n\r\n     <banner :listImg=\"adsImg\" myClass=\"-c2\"></banner>\r\n\r\n      <!-- 提交遮罩 --> \r\n    <alert-tip v-show=\"showAlertTip\" :formLoading=\"formLoading\" :alertText=\"alertText\"></alert-tip>\r\n  </div> \r\n</template>\r\n\r\n<script>\r\nimport alertTip from \"/components/alertTip\"\r\nimport Banner from '/components/swiperDefault'\r\nexport default {\r\n  name: \"redPacket\",\r\n  data() {\r\n    return {\r\n       showAlertTip: false,\r\n       formLoading: false,\r\n       ebiTotal: 0,//ebi总额\r\n       currentEbi: 0,//当前快过期e币\r\n       currentEbuy: 0,//当前快过期e购\r\n       ebiInput: '',\r\n       ebuyInput: '',\r\n       redpacket: 0\r\n    };\r\n  },\r\n  components: {\r\n    alertTip, Banner\r\n  },\r\n  created() {\r\n      document.title = \"红包充值\";\r\n      this._initData();\r\n  },\r\n  computed: {\r\n    getRedPacket() {\r\n      if(this.ebiInput != '' && this.ebuyInput != '') {\r\n          this.redpacket = parseInt(this.ebiInput * 0.3 + this.ebuyInput * 0.7);\r\n      }\r\n      return this.redpacket;\r\n    }\r\n  },\r\n  methods: {\r\n    _initData() {\r\n      // 轮播图2\r\n      banner({params:{location: \"eticket_recharge\"}}).then(res => {\r\n          this.adsImg = res.obj;\r\n      }).then(() => {\r\n          var swiperc1 = new Swiper('.swiper-container-c2', {\r\n              slidesPerView: 'auto',\r\n              centeredSlides: true,\r\n              paginationClickable: true,\r\n              spaceBetween: 16,\r\n              loop: true,\r\n              autoplay: 5000,\r\n              autoplayDisableOnInteraction: false\r\n          });\r\n      })\r\n    },\r\n    //点击兑换按钮\r\n    gotoExchange() {\r\n      if(this.ebiInput > this.ebiTotal || this.ebuyInput > this.currentEbuy) {\r\n        this.showHideAlert('可兑换的e币或e购不能大于总额');\r\n      } else {\r\n        this.showAlertTip = true;//提交中提示\r\n        this.formLoading = true;\r\n        this.alertText = '提交中，请稍候';\r\n      }\r\n    },\r\n     //显示隐藏提示框\r\n    showHideAlert(text) { \r\n        this.showAlertTip = true;\r\n        this.formLoading = false;\r\n        this.alertText = text;\r\n        setTimeout(() => {\r\n            this.showAlertTip = false;\r\n        }, 2000);\r\n    }\r\n      \r\n  }\r\n};\r\n</script>\r\n<style lang=\"scss\" scoped>\r\n@import '../../assets/scss/var.scss';\r\n .redpacket-top {\r\n    background: $bkg-theme;\r\n    