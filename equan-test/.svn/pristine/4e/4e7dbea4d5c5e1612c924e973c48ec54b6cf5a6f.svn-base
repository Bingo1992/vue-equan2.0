mE,EAAA6B,OAAAL,WAAkClC,EAAAW,GAAA,KAAAR,EAAA,KAAsBE,YAAA,mBAAyBL,EAAAW,GAAA,KAAAR,EAAA,OAA4BE,YAAA,QAAkBF,EAAA,KAAUE,YAAA,YAAAG,IAA4BC,MAAAT,EAAAP,aAAuBO,EAAAW,GAAA,YAAAX,EAAAyB,OAAAzB,EAAAW,GAAA,KAAAX,EAAA,aAAAG,EAAA,aAA+EG,OAAOmC,aAAAzC,EAAAJ,aAA4BI,EAAAyB,KAAAzB,EAAAW,GAAA,KAAAX,EAAAgB,GAAA,QAEzlH0B,iBADjB,WAAoC,IAAazC,EAAb3C,KAAa4C,eAA0BC,EAAvC7C,KAAuC8C,MAAAD,IAAAF,EAAwB,OAAAE,EAAA,SAAAA,EAAA,MAAAA,EAAA,MAA/D7C,KAA+DqD,GAAA,UAA/DrD,KAA+DqD,GAAA,KAAAR,EAAA,MAA/D7C,KAA+DqD,GAAA,UAA/DrD,KAA+DqD,GAAA,KAAAR,EAAA,MAA/D7C,KAA+DqD,GAAA,eAAsI,WAAc,IAAaV,EAAb3C,KAAa4C,eAA0BC,EAAvC7C,KAAuC8C,MAAAD,IAAAF,EAAwB,OAAAE,EAAA,OAAiBE,YAAA,gBAA0BF,EAAA,KAAUG,OAAOqC,KAAA,kBAAuBxC,EAAA,KAAUE,YAAA,oBCEnZ,IAcAuC,EAdAhB,EAAA,OAcAiB,CACAhH,EACAiE,GATA,EAVA,SAAAgD,GACAlB,EAAA,SAaA,kBAEA,MAUAmB,EAAA,QAAAH,EAAA","file":"static/js/15.22556e5b134fefeb5793.js","sourcesContent":["<template>\n\t<div id=\"accountList\" class=\"banner-b\">\n\t\t<div class=\"credit-top\">\n\t        <div class=\"search\" @click=\"showDialog = !showDialog\">\n\t            <i class=\"icon-search\"></i>\n\t            <span>按日期查询</span>\n\t        </div>\n\t        <div class=\"credit-num\">\n\t            <p>e{{eIndex === 0? '币':'购'}} 总额</p>\n\t            <h1>{{eIndex === 0? ebi:ebuy}}</h1>\n\t        </div>\n\t        <router-link class=\"btn-recharge\" :to=\"{path:'/ticket',query:{tckType:1}}\">e{{eIndex === 0? '币':'购'}}充值</router-link>\n\t    </div>\n\n\t    <!-- 输入查询日期结束 -->\n\t    <article class=\"bg-mgShow\">\n\t        <div class=\"credit-title border-bottom\">\n\t            <p>{{ beginTime != '' && endTime != '' ? beginTime1 + '至' + endTime1 :'7天内'}}交易情况</p>\n\t        </div>\n\t        <table class=\"credit-detail border-bottom\">\n\t            <thead>\n\t                <tr>\n\t                    <th>交易日期</th>\n\t                    <th>交易描述</th>\n\t                    <th>额度变动</th>\n\t                </tr>\n\t            </thead>\n\t            <tbody>\n\t                <tr v-for=\"(item, i) in accountList\" :key=\"i\">\n\t                    <td>{{item.opTime}}</td>\n\t                    <td>{{item.remark}}</td>\n\t                    <td v-if=\"eIndex === 0\" :class=\"[item.opType==1?'plus':'minus']\">{{item.opType==1?'+':'-'}}{{item.opVal}}</td>\n\t                    <td v-if=\"eIndex === 1\" :class=\"[item.opType==1?'plus':'minus']\">{{item.opType==1?'+':'-'}}{{item.opEbuyVal}}</td>\n\t                </tr>\n\t               <!--  <tr>\n\t                    <td>2015-12-24</td>\n\t                    <td>加油服务</td>\n\t                    <td class=\"plus\">+100</td>\n\t                </tr>\n\t                <tr>\n\t                    <td>2015-12-24</td>\n\t                    <td>中石化燃油宝</td>\n\t                    <td class=\"minus\">-20</td>\n\t                </tr> -->\n\t            </tbody>\n\t        </table>\n\t        \n\t        <!-- 加载更多 -->\n\t\t\t<load-more v-if=\"loadmore\" :isLoading=\"isLoading\" :noData=\"noData\" @loadMore=\"loadMore\"></load-more>\n\n\t\t\t<div v-if=\"accountList.length==0 && !isLoading\" class=\"nothing\">\n\t\t        <img src=\"../../assets/images/nothing.png\" >\n\t\t        <p class=\"font-gray\">暂无数据</p> \n\t\t    </div>\n\t    </article>\n\t    \n\t\t<div class=\"fixed-bottom\">\n\t        <ul class=\"flex-layout nav-bar border-top\">\n\t            <li :class=\"['border-right', eIndex===i? 'active':'']\" v-for=\"(item, i) in eList\" @click=\"getOtherList(i)\" :key=\"i\">\n\t                <a>{{item}}</a>\n\t            </li>\n\t           <!--  <li>\n\t                <a>e购明细</a>\n\t            </li> -->\n\t        </ul>\n\t    </div>\n\n\n\t    <!-- 输入查询日期 -->\n\t    <transition name=\"router-slid\" mode=\"out-in\">  \n\t\t    <div v-if=\"showDialog\" class=\"time-list fixed-body\">\n\t            <a class=\"list-box border-bottom\" @click=\"showDialog = !showDialog\">\n\t                <i class=\"icon-left\"></i> \n\t                <span class=\"font-gray\">返回</span>\n\t            </a>\n\t            <ul class=\"border-list\">\n\t                <li>\n\t                    <label for=\"beginTime\">选择开始时间</label>\n\t                    <input type=\"date\" name=\"beginTime\" placeholder=\"请输入查询时间段\" v-model=\"beginTime\">\n\t                    <i class=\"icon-right\"></i>\n\t                </li>\n\t                <li>\n\t                    <label for=\"endTime\">选择结束时间</label>\n\t                    <input type=\"date\" name=\"endTime\" placeholder=\"请输入查询时间段\" v-model=\"endTime\">\n\t                    <i class=\"icon-right\"></i>\n\t                </li>\n\t            </ul>\n\t            <div class=\"btn\">\n\t            \t<p class=\"btn-theme\" @click=\"checkData\">查询</p>\n\t            </div>\n\t\t    </div>\n\t\t</transition>\n\n\t\t <!-- 提示弹窗 -->\n        <alert-tip v-if=\"showAlertTip\" :alert-text=\"alertText\"></alert-tip>\n          \n\t\t\n       <!-- 返回顶部按钮 -->\n      <div class=\"fixed-right\">\n        <a href=\"#accountList\"><i class=\"icon-top\"></i></a>\n      </div>\n\n\t</div>\n</template>\n<script>\nimport {account, integral} from '/api/api'\nimport alertTip from '/components/alertTip'\nimport loadMore from '/components/loadMore'\n\nexport default {\n\tdata() {\n\t\treturn {\n\t\t\tebi: 0,//e币总和\n\t\t\tebuy: 0,//e购总和\n\t\t\teIndex: 0,//0为e币，1为e购\n\t\t\teList: ['e币明细', 'e购明细'],\n\t\t\taccountList: [],// 账户列表account\n\t\t\tshowDialog: false,//是否显示遮罩\n\t\t\tshowAlertTip: false,//是否显示弹窗\n\t\t\tbeginTime: '',//input开始时间\n\t\t\tendTime: '',//input结束时间\n\t\t\tbeginTime1: '',//确认后的开始时间\n\t\t\tendTime1: '',//确认后的结束时间\n\t\t\tqueryType: 'ecoin',\n\t\t\tisLastPage: false,\n\t\t\tpage: 1,\n\t\t\tloadmore: true,//显示加载更多\n\t\t\tisLoading: true,\n\t\t\tnoData: false\n\t\t}\n\t},\n\tcomponents: {\n        alertTip, loadMore\n    },\n    mounted() {\n    \tdocument.title = \"我的账户\";\n    \tthis._initData();\n\t\tthis.$nextTick(() => {\n\t\t\twindow.scrollTo(0,1);\n\t\t\twindow.scrollTo(0,0);\n\t\t});\n\t\n    },\n    methods: {\n    \t_initData() {\n    