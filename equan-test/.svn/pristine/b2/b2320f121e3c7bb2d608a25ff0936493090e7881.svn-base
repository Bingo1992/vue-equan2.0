div>\r\n\r\n\t\t\t<div v-if=\"esm.flag || recharge.flag || oil.flag\" class=\"list-box border-bottom\">\r\n\t\t\t\t<img class=\"list-img-small\" :src=\"getUrlPath(typeList[type].src)\">\r\n\t\t\t \r\n\t\t\t\t<div class=\"list-info-v\">\r\n\t\t\t\t\t<h5  class=\"nowrap-2\">{{typeList[type].name}}</h5>\r\n\t\t\t\t\t<p v-if=\"type == 4 || type == 6 || type == 9\">{{orderName}}</p>\r\n\t\t\t\t\t<p v-else>金额：{{amount}} </p>\r\n\t\t\t\t</div>\r\n\t\t\t\t<p>\r\n\t\t\t\t\t<span v-if=\"type == 1 || type == 13\">{{ecoin+'e币'}}</span>\r\n\t\t\t\t\t<span v-else-if=\"type == 2\">{{'￥'+redPacket}}</span>\r\n\t\t\t\t\t<span v-else-if=\"type == 10\">{{ecoin + 'e币+' + ebuy+'e购'}}</span>\r\n\t\t\t\t\t<span v-else-if=\"type == 11\">{{'￥'+redPacket + '+' + ebuy+'e购'}}</span>\r\n\t\t\t\t\t<span v-else-if=\"type == 12\">{{ebuy+'e购'}}</span>\r\n\t\t\t\t\t<span v-else>￥{{amount}}</span>\r\n\t\t\t\t</p>\r\n\t\t\t</div>\r\n\r\n\t\t\t<ul v-if=\"esm.flag\" class=\"order-detail\">\r\n\t\t\t\t<li>\r\n\t\t\t\t\t<span>商品总额：</span>\r\n\t\t\t\t\t<span class=\"fr\">￥{{amount}}</span>\r\n\t\t\t\t</li>\r\n\t\t\t\t<li v-if=\"redPacket != 0\">\r\n\t\t\t\t\t<span>发放红包：</span>\r\n\t\t\t\t\t<span class=\"fr\">{{redPacket}}</span>\r\n\t\t\t\t</li>\r\n\t\t\t\t<li v-if=\"ebuy != 0\">\r\n\t\t\t\t\t<span>发放e购：</span>\r\n\t\t\t\t\t<span class=\"fr\">{{ebuy}}</span>\r\n\t\t\t\t</li>\r\n\t\t\t\t<li v-if=\"ecoin != 0\">\r\n\t\t\t\t\t<span>发放e币：</span>\r\n\t\t\t\t\t<span class=\"fr\">{{ecoin}}</span>\r\n\t\t\t\t</li>\r\n\t\t\t</ul> \r\n\r\n\t\t\t<!-- 充值 + 实物-->\t\r\n\t\t\t<ul v-else class=\"order-detail\">    \r\n\t\t\t\t<li class=\"list-box\">\r\n\t\t\t\t\t<span>商品总额：</span>\r\n\t\t\t\t\t<span class=\"list-info-h flex-end\">￥{{amount}}</span>\r\n\t\t\t\t</li>\r\n\t\t\t\t<li v-if=\"ecoin != 0\" class=\"list-box\">\r\n\t\t\t\t\t<span>e币支付：</span>\r\n\t\t\t\t\t<span class=\"list-info-h flex-end\">{{ecoin}}</span>\r\n\t\t\t\t</li>\r\n\t\t\t\t<li v-if=\"money != 0\" class=\"list-box\">\r\n\t\t\t\t\t<span>微信支付：</span>\r\n\t\t\t\t\t<span class=\"list-info-h flex-end\">￥{{money}}</span>\r\n\t\t\t\t</li>        \r\n\t\t\t</ul>\r\n\t\t</div>\t\t\r\n\t\r\n\t\t<ul class=\"order-detail bg-mgShow\">\r\n\t\t\t<li>\r\n\t\t\t\t<span class=\"pdr\">订单编号：</span>\r\n                <span>{{orderId}}</span>\r\n\t\t\t</li>\r\n\t\t\t<li>\r\n\t\t\t\t<span class=\"pdr\">下单时间：</span>\r\n                <span>{{time}}</span>\r\n\t\t\t</li>\r\n\t\t</ul>\r\n\r\n\t\t\r\n\t\t\r\n\t</div>\r\n\r\n</template>\r\n<script>\r\nimport {orderDetail} from '/api/api'\r\nimport alertTip from '/components/alertTip'\r\nimport product from '/components/product'\r\nimport loading from '/components/loading'\r\nimport loadMore from '/components/loadMore'\r\nimport { getImgPath, getUrlPath } from '/components/mixin'\r\nexport default {\r\n\tname: 'orderList',\r\n\tdata() {\r\n\t\treturn {\r\n\t\t\torderId: '',\r\n\t\t\ttime: '',\r\n\t\t\tdetail: {},//详情内容\r\n\t\t\tamount: 0,//订单金额\r\n\t\t\tecoin: 0,//e币\r\n\t\t\tebuy: 0,//e购\r\n\t\t\tredPacket: 0,//红包\r\n\t\t\tmoney: 0,\r\n\t\t\tstate: 0,//订单状态\r\n\t\t\torderName: '-',\r\n\t\t\tesm: {\r\n                flag: false,\r\n\t\t\t\tesmNum: '',//卡券号码  \r\n\t\t\t\tname: ''\r\n            },\r\n            oil: {\r\n                flag: false,\r\n                oilCardNum: '',//油卡号\r\n                oilAmount: '',//充值金额\r\n                IDcard: '',//身份证号\r\n                type: '',//油卡类型\r\n\t\t\t\tname: '',//持卡人姓名\r\n\t\t\t\t\r\n            },  \r\n            recharge: {\r\n\t\t\t\tflag: false,\r\n\t\t\t\tmediaFlag: false,\r\n\t\t\t\tmobileNum: '',//手机号 \r\n\t\t\t\tmediaNum: ''//充值账号\r\n            },\r\n            pro: {\r\n                flag: false,\r\n\t\t\t\tname: '',\r\n\t\t\t\tmobile: '',\r\n\t\t\t\taddress: '',\r\n\t\t\t\tlist: []\r\n\t\t\t},\r\n\t\t\ttype: 1,\r\n\t\t\tstateList: {\r\n\t\t\t\t0: {\r\n\t\t\t\t\t0: '未支付',\r\n\t\t\t\t\t1: '已发货，待支付',\r\n\t\t\t\t\t2: '已发货',\r\n\t\t\t\t\t3: '已签收',\r\n\t\t\t\t\t4: '已退款'\r\n\t\t\t\t},\r\n\t\t\t\t1: {\r\n\t\t\t\t\t1: '兑换成功'\