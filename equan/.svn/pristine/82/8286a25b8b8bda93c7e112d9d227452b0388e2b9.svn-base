,KACAqB,EAAA1F,UAAAsE,UAAsCoB,EAAAU,GAAA,UAAAV,EAAAa,KAAAb,EAAAU,GAAA,QAAAV,EAAAvF,MAAA0F,EAAA,KAA+DE,YAAA,0BAAoCL,EAAAU,GAAA,cAAAV,EAAAxF,MAAA2F,EAAA,KAA2CE,YAAA,0BAAoCL,EAAAU,GAAA,WAAAV,EAAAa,MAAA,KAAAb,EAAAU,GAAA,KAAAV,EAAA,aAAAG,EAAA,aAAkFG,OAAOyB,aAAA/B,EAAAnF,aAA4BmF,EAAAa,MAAA,IAE5TmB,oBCXjB,IAcAC,EAdyBrB,EAAQ,OAcjCsB,CACE9H,EACA0F,GATF,EAVA,SAAAqC,GACEvB,EAAQ,SAaV,kBAEA,MAUewB,EAAA,QAAAH,EAAiB","file":"static/js/11.164719f4b2ef043d7144.js","sourcesContent":["module.exports = \"data:image/png;base64,\"\n\n\n//////////////////\n// WEBPACK FOOTER\n// ./src/assets/images/state.png\n// module id = BMzt\n// module chunks = 11","<template>\n    <div class=\"children-view\">\n        <loading v-if=\"showLoading\"></loading>\n        <section v-else class=\"content-main\">\n            <!-- 图片轮播 -->\n            <banner :listImg=\"imglistSrc\" myClass=\"1\"  hasPoint=\"true\"></banner>\n            <div v-if=\"state==0\" class=\"state-down\">\n                <img src=\"../../../assets/images/state.png\">\n            </div>\n            <div :key=\"proDetail.id\">\n                <div class=\"pro-list pdl pdr pdb\">\n                   <!--  <div class=\"label\">\n                        <span v-if=\"proDetail.label1\" class=\"label-orange\">{{proDetail.label1}}</span>\n                        <span v-if=\"proDetail.label2\" class=\"label-blue\">{{proDetail.label2}}</span>\n                        <span v-if=\"proDetail.label3\" class=\"label-red\">{{proDetail.label3}}</span>\n                    </div>\n                     -->\n                    <h4 class=\"pro-title\">\n                    <!-- <span v-if=\"proDetail.ebuy == 1\" class=\"label-red\">支持e购</span>&nbsp; -->\n                    {{proDetail.name}}\n                    </h4>\n   \n                    <!-- <div v-if=\"proDetail.areaId && (item.areaId == proDetail.areaId[0])\" class=\"label\" \n                    v-for=\"(item, i) in activityList\" :key=\"i\">\n                        <router-link class=\"label-orange\" v-for=\"(label, j) in item.labelList\" :key=\"j\" \n                        :to=\"{path:'/productDetail',query:{productid: label.id}}\">{{label.label}}</router-link>\n                    </div> -->\n                     <div v-if=\"hasActivityAreaId\" class=\"label\">\n                        <router-link class=\"label-orange\" v-for=\"(label, j) in labelList\" :key=\"j\" \n                        :to=\"{path:'/productDetail',query:{productid: label.id}}\">{{label.label}}</router-link>\n                    </div>\n                    <div class=\"price\">\n                        <span class=\"font-orange lg-font\">\n                            <i class=\"icon-coin\"></i>\n                            {{proDetail.cost}}\n                        </span>\n                        <s class=\"right-text font-gray\">市场价￥{{proDetail.price}}</s>\n                    </div>\n                </div>   \n                <div class=\"bg-mgShow pro-detail\">\n                    <h4 class=\"center-text\">商品详情</h4>\n                    <img v-for=\"(item,i) in proDetail.detailImg\" :src=\"getImgPath(item.showPic)\" :key=\"i\">\n                    <div class=\"pdb pdr pdl\" v-html=\"proDetail.description\"></div>\n                </div>\n            </div>\n\n            <div style=\"padding-bottom:50px\"></div>\n       \n        </section>\n\n        <div class=\"fixed-bottom bg-gray\">\n            <div class=\"bottomBtn two-btn border-top\">\n                <router-link class=\"backNav border-right\" to=\"/cart\">\n                    <i class=\"icon-cart\"></i>\n                    <!-- <span v-if=\"cartNum != 0\" class=\"circlePoint\">{{cartNum}}</span> -->\n                    <cart-num></cart-num>\n                </router-link>\n\n                <a v-if=\"count != 0 && state==1\" class=\"btn-cart\" @click=\"addToCart(\n                    proDetail.showPic, \n                    proDetail.name, \n                    proDetail.cost, \n                    proDetail.price, \n                    proDetail.ebuy,\n                    proDetail.type)\">加入购物车</a>\n                <a v-if=\"count != 0  && state==1\" @click=\"goToBuy(\n                    proDetail.showPic, \n                    proDetail.name, \n                    proDetail.cost, \n                    proDetail.price, \n                    proDetail.ebuy,\n                    proDetail.type)\" class=\"btn-theme\">立即购买</a>\n                <a v-if=\"state==0\" class=\"btn-pure-theme disbtn\">商品已下架</a> \n                <a v-else-if=\"count==0\" class=\"btn-pure-theme disbtn\">商品已售罄</a>\n            </div>\n        </div>\n\n         <!-- 提示遮罩 -->\n        <alert-tip v-if=\"showAlertTip\" :alert-text=\"alertText\"></alert-tip>\n          \n    </div>\n    \t \n\n</template>\n\n<script>\nimport {mapMutations} from 'vuex'\nimport {proDetail, addCart} from '/api/api'\nimport { setStore } from '/utils/storage'\nimport {getImgPath} from '/components/mixin'\nimport alertTip from '/components/alertTip'\nimport Banner from '/components/swiperDefault'\nimport Loading from '/components/loading'\nimport cartNum from '/components/cartNum'\n\nexport default {\n\tdata(){\n\t\treturn {\n\t\t\tproDetail: [],\n\t\t\timglistSrc:[],//轮播图\n            count: 1,//商品：1未售罄，0已售罄\n            state: 1,//商品：1上架，0下架\n            showLoading: true,\n            showAttrDialog: false,\n            showAlertTip: false,\n            alertText: \"\",\n            hasActivityAreaId: false,//是否参与活动\n            labelList: [],\n            activityList:[{\n                \"areaId\": 132,\n                \"labelList\": [{\n                    \"id\": 6,\n                    \"label\": \"下单赠品二选一手机支架\"\n                }, {\n                    \"id\": 1341,\n                    \"label\": \"或雨刷精\"\n                }]\n            },{\n                \"areaId\": 133,\n                \"labelList\": [{\n                    \"id\": 118,\n                    \"label\": \"满200送哆啦A梦自拍杆\"\n                }]\n            },{\n                \"areaId\": 134,\n                \"labelList\": [{\n                    \"id\": 578,\n                    \"label\": \"下单送OOU水果刀\"\n                }]\n            },{\n                \"areaId\": 135,\n                \"labelList\": [{\n                    \"id\": 638,\n                    \"label\": \"下单送厨房三件套\"\n                }]\n            },{\n                \"areaId\": 136,\n                \"labelList\": [{\n                    \"id\": 577,\n                    \"label\": \"下单送三层餐盒\"\n                }]\n            },{\n                \"areaId\": 137,\n                \"labelList\": [{\n                    \"id\": 937,\n                    \"label\": \"下单送蛋糕机\"\n                }]\n            },{\n                \"areaId\": 138,\n                \"labelList\": [{\n                    \"id\": 827,\n                    \"label\": \"满398送菲仕堡红酒\"\n                }]\n            },{\n                \"areaId\": 139,\n                \"labelList\": [{\n                    \"id\": 628,\n                    \"label\": \"满200送淮山莲子面\"\n                }]\n            },{\n                \"areaId\": 140,\n                \"labelList\": [{\n                    \"id\": 725,\n                    \"label\": \"满249送陪伴枕\"\n                }, {\n                    \"id\": 711,\n                    \"label\": \"满499送雪绒毯\"\n                }]\n            },{\n                \"areaId\": 141,\n                \"labelList\": [{\n                    \"id\": 1343,\n                    \"label\": \"下单不满100送杯刷\"\n                }, {\n                    \"id\": 1342,\n                    \"label\": \"满100送杯渍清洁剂\"\n                }]\n            },{\n                \"areaId\": 142,\n                \"labelList\": [{\n                    \"id\": 352,\n                    \"label\": \"下单送啄木鸟短款钱夹\"\n                }]\n            },{\n                \"areaId\": 143,\n                \"labelList\": [{\n                    \"id\": 164,\n                    \"label\": \"下单送丸美眼霜\"\n                }]\n            },{\n                \"areaId\": 144,\n                \"labelList\": [{\n                    \"id\": 613,\n                    \"label\": \"下单送舒适剃须刀\"\n                }]\n            },{\n                \"areaId\": 145,\n                \"labelList\": [{\n                    \"id\": 742,\n                    \"label\": \"下单送好易康旅行装\"\n                }]\n            },{\n                \"areaId\": 146,\n                \"labelList\": [{\n                    \"id\": 198,\n                    \"label\": \"下单送6片WIS面膜\"\n                }]\n            }]\n           \n\t\t}\n\t},\n    components: {\n        Banner, Loading, alertTip, cartNum\n    },\n\tmounted() {\n        document.title = \"商品详情\";\n\t\tthis._initData();\n    },\n    mixins: [getImgPath],\n    // computed: {\n    //     ...mapState(['cartList']),\n    //     cartNum () {\n    //       let cartNum = 0;\n    //       this.cartList && this.cartList.forEach( item => {\n    //          cartNum += item.proNum;\n    //       })\n    //       return cartNum;\n    //     }\n    // },\n\tmethods: {  \n        ...mapMutations(['ADD_CART']),\n\t\t_initData() {\n            var self = this;\n            // document.title = \"商品详情\";\n            // console.log(document.title)\n            proDetail({params:{productid: this.$route.query.productid}}).then(res => {\n                this.proDetail = res;\n                this.count = this.proDetail.count;\n                this.state = this.proDetail.state;\n                this.showLoading = false;\n                let pic = this.proDetail.pic;   \n                pic&&pic.forEach( function(element, index) {\n                    self.imglistSrc.push({'pic':element});//图片转换\n                });\n                //参与双十一活动的专区商品判断\n                this.proDetail.areaId && this.proDetail.areaId.forEach((areaId, i) => {\n                    this.activityList.forEach((label, j) => {\n 