       <h4>人气推荐</h4>\n        <i class=\"icon-circle\"></i>\n      </router-link>\n      <product :proList=\"proList2\"></product>\n  </div> \n</template>\n\n<script>\n    import {proList} from '/api/api'\n    import { getUrlPath } from '/components/mixin'\n    import product from '/components/product'\n    // import '/utils/jweixin-1.0.0.js'\n    export default {\n      name: 'pay',\n      data () {\n        return {\n           proList2: [],\n           type: 0,//兑换类型\n           ebi: 0,//获得e币\n           ebuy: 0,//e购\n           redPacket: 0//红包\n        }\n      },\n      components: {\n       product\n      }, \n      mounted() {\n         this._initData();\n      },\n      mixins: [getUrlPath],\n      methods: {\n         _initData() {\n            this.type = Number(this.$route.query.type);\n            this.ebi = this.$route.query.ecoin;\n            this.ebuy = this.$route.query.ebuy;\n            this.redPacket = this.$route.query.redPacket;\n            if(this.type) {\n              document.title = \"兑换结果\";\n            } else {\n              document.title = \"支付结果\";\n            }\n            //商品列表(推荐)\n            proList({params:{areaId: 2,pageSize: 30}}).then(res => {\n              this.proList2 = res.attributes.resultList;\n            });\n         }\n        \n      }\n\n    }\n</script>\n\n<!-- Add \"scoped\" attribute to limit CSS to this component only -->\n<style lang=\"scss\" scoped>\n.two-btn {\n  padding: 0 10px;\n  a {\n    margin: 0 10px;\n  }\n}\n \n</style>\n\n\n\n// WEBPACK FOOTER //\n// src/pages/order/result.vue","var render = function () {var _vm=this;var _h=_vm.$createElement;var _c=_vm._self._c||_h;return _c('div',[_c('div',{staticClass:\"result\",staticStyle:{\"padding-bottom\":\"1rem\"}},[_c('i',{staticClass:\"icon-hook bg-success\"}),_vm._v(\" \"),(this.type != 0)?_c('h4',[_vm._v(\"兑换成功\")]):_c('h4',[_vm._v(\"支付成功\")])]),_vm._v(\" \"),_c('div',{staticClass:\"rich-box font-gray\"},[(_vm.type == 1)?_c('p',{staticClass:\"c